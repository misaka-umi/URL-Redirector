{
  "manifest_version": 3,
  "name": "URL Redirector",
  "version": "1.0",
  "description": "Redirect specified URLs to target URLs",
  "permissions": [
    "storage",
    "webNavigation"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
